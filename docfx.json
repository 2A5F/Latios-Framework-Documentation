{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "LatiosFramework/Latios.**.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "**.md",
          "**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "**.md",
          "**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "src": "../LatiosFramework/Packages/com.latios.latiosframework/"
      }
    ],
    "resource": [
      {
        "files": [
          "media/**",
          "**/media/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern"],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Latios Framework",
      "_appName": "Latios Framework",
      "_appFaviconPath": "media/logo_bad.png",
      "_appLogoPath": "media/logo_bad.png",
      "_appFooter": "Got questions? Reach out any time!",
      "_copyrightFooter": "The Latios Framework is licensed under the Unity Companion License.",
      "_enableSearch": true,
      "_disableSideFilter": false,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_disableBreadcrumb": false,
    },
    "sitemap": {
      "baseUrl": "https://latios-framework.github.com",
      "priority": 0.1,
      "changefreq": "weekly"
    }
  }
}